{
    "manifest_version": 2,
    "name": "Feedback Accessiblity Extension",
    "description": "Submits accessiblity feedback from user to website admin",
    "version": "1.0.0",
    "icons":{"128": "icon_128.png"},
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]

}